JAL :
 0 => "0011011110110101",  --R1<- 1010101010101010
 1 => "1000001000000110",   -- R1<- 0010010000000000
 --2 => "0000011001110000",   -- R4<- R2+R1
 --3 => "0000000000000000",   -- R6<- 0001111000000000
 7 => "0011101111111111",
 --5 => "0000000000000000",
 --6 => "0000000000000000",
 --7 => "0000000000000000",
 others => "1111000000000000"

